msgid "policy_notification.sms_after_expiry"
msgstr "La póliza para la familia/grupo %(InsuranceID)s - %(Name)s con el producto de seguro %(ProductCode)s - %(ProductName)s expiró el %(ExpiryDate)s y aún no ha sido renovada. Se recomienda su renovación."

msgid "policy_notification.sms_on_renewal"
msgstr "La póliza para la familia/grupo %(InsuranceID)s - %(Name)s con el producto de seguro %(ProductCode)s - %(ProductName)s ha sido renovada desde la fecha %(EffectiveDate)s hasta la fecha de vencimiento %(ExpiryDate)s ."

msgid "policy_notification.sms_control_number_assigned"
msgstr "El número de control %(ControlNumber)s fue asignado a la solicitud emitida el día %(DateRequest)s a %(TimeRequest)s para el pago del conjunto de pólizas que contiene la familia/grupo %(InsuranceID)s - %(Name)s con el producto de seguro %(ProductCode)s - %(ProductCode)s y en conjunto las pólizas %(NumberPolicies)s . El importe pago asociado con la solicitud es %(AmountToBePaid)s ."

msgid "policy_notification.sms_control_number_error_bulk_payment"
msgstr "No se puede asignar el número de control %(ControlNumber)s para la solicitud emitida el día %(DateRequest)s a %(TimeRequest)s para el pago del conjunto de pólizas que contiene la familia/grupo %(InsuranceID)s - %(Name)s con el producto de seguro %(ProductCode)s - %(ProductCode)s y en conjunto las pólizas %(NumberPolicies)s . El motivo es %(ErrorMessage)s ."

msgid "policy_notification.sms_control_number_error_single_payment"
msgstr "No se puede asignar el número de control %(ControlNumber)s para la solicitud emitida el día %(DateRequest)s a %(TimeRequest)s para el pago de la póliza de la familia/grupo %(InsuranceID)s con el producto de seguro %(ProductCode)s . El motivo es %(ErrorMessage)s"

msgid "policy_notification.sms_paid_and_activated"
msgstr "El pago %(Payment)s realizado el %(DatePayment)s para el número de control %(ControlNumber)s recibido y la póliza para la familia/grupo %(InsuranceID)s - %(Name)s con el producto de seguro %(ProductCode)s - %(ProductName)s activado desde la fecha %(EffectiveDate)s hasta la fecha de vencimiento %(ExpiryDate)s . El pago asignado para la póliza es %(Payment)s ."

msgid "policy_notification.sms_paid_and_not_activated"
msgstr "El pago %(Payment)s realizado el %(DatePayment)s para el número de control %(ControlNumber)s recibido pero la póliza para la familia/grupo %(InsuranceID)s - %(Name)s con el producto de seguro %(ProductCode)s - %(ProductName)s no se ha activado. El pago asignado para la póliza es %(Payment)s pero aún así se debe pagar %(PaymentLeft)s para la activación de la póliza."

msgid "policy_notification.sms_paid_and_not_matched"
msgstr "El pago %(Payment)s realizado el %(DatePayment)s para el número de control %(ControlNumber)s recibido, pero las pólizas en el conjunto que contiene la familia/grupo %(InsuranceID)s - %(Name)s con el producto de seguro %(ProductCode)s - %(ProductName)s no puede coincidir por el momento"

